<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <application>
        <locale-config>
            <default-locale>
                en_CA
            </default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>/TraduccionesGenerales</base-name>
            <var>TraduccionesGenerales</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Inicio</base-name>
            <var>Inicio</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Layout</base-name>
            <var>Layout</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Login</base-name>
            <var>Login</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Crear</base-name>
            <var>Crear</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Estadisticas</base-name>
            <var>Estadisticas</var>
        </resource-bundle>
    </application>
</faces-config>